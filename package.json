{
  "name": "typewriter-editor",
  "version": "0.11.0",
  "description": "A rich text editor using the Delta format with decorations and rendered with a tiny virtual dom",
  "keywords": [
    "typewriter",
    "rich text editor",
    "wysiwyg",
    "svelte",
    "delta"
  ],
  "scripts": {
    "build": "tsc && cp src/*.svelte",
    "start": "webpack serve --config webpack.dev.config.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typewriter-editor/typewriter.git"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@typewriter/document": "^0.8.0"
  },
  "peerDependencies": {
    "svelte": ">=3.43.0 <5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@tsconfig/svelte": "^5.0.4",
    "@types/fast-diff": "^1.2.0",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "jsdom-global": "^3.0.2",
    "raw-loader": "^4.0.2",
    "svelte": "^4.2.18",
    "svelte-loader": "^3.2.3",
    "svelte-preprocess": "^6.0.1",
    "svelte-routing": "^1.11.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
